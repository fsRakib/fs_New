   // deletes all the vowels
    for(i=0; i<l; i++)
    {
        if(str[i]=='a' || str[i]=='o' || str[i]=='y' || str[i]=='e' || str[i]=='u' || str[i]=='i')
        {
            for(j=i; j<l; j++)
            {
                str[j] = str[j+1];
            }
            l--;
             i--;

        }

    }
